<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="team3.swS.lvoca.main.AnotherWordBookController">
   <center>
      <VBox fx:id="popWordBook" prefHeight="150.0" prefWidth="370.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="tableviewFile" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="fileNameCol" prefWidth="369.0" text="선택한 단어장" />
              </columns>
            </TableView>
         </children></VBox>
   </center>
   <bottom>
      <Button fx:id="addLvoca" mnemonicParsing="false" onAction="#addAllLvoca" text="합치기 실행" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
